Host *
    ControlMaster auto
    ControlPath ~/.ssh/controlmasters/%C
    # Persist for 5 seconds after killing master session, so we can
    # immediately re-log-in if needed
    ControlPersist 5

Host qxlp
    User gunsch
    Hostname qxlp.net
    Port 37912

Host home
    User jesse
    Hostname home.ns.qxlp.net
    Port 37912

Host 192.168.0.101
    IdentitiesOnly yes

